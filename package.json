{
  "name": "wrap-royale",
  "version": "1.1.1",
  "description": "A promise based, feature-rich library for requestion resources from Supercell's official Clash Royale API, written in TypeScript.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json --format stylish src/**/*.ts",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "test:arena-helper": "mocha -r ts-node/register src/**/arena-helper.spec.ts",
    "test:badge-helper": "mocha -r ts-node/register src/**/badge-helper.spec.ts",
    "test:card-helper": "mocha -r ts-node/register src/**/card-helper.spec.ts",
    "test:hashtag-helper": "mocha -r ts-node/register src/**/hashtag-helper.spec.ts",
    "test:locale-helper": "mocha -r ts-node/register src/**/locale-helper.spec.ts",
    "test:rarity-helper": "mocha -r ts-node/register src/**/rarity-helper.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weeco/wrap-royale.git"
  },
  "keywords": [
    "clash",
    "royale",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "Weeco <weeco91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weeco/wrap-royale/issues"
  },
  "homepage": "https://github.com/weeco/wrap-royale#readme",
  "dependencies": {
    "bases": "^0.2.1",
    "long": "^4.0.0",
    "moment": "^2.20.1",
    "reflect-metadata": "^0.1.10",
    "slug": "^0.9.1",
    "wrap-royale-core": "^1.0.5"
  },
  "devDependencies": {
    "@types/bases": "^0.2.2",
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-json-schema": "^1.4.3",
    "@types/dotenv": "^4.0.2",
    "@types/joi": "^13.0.6",
    "@types/long": "^3.0.32",
    "@types/mocha": "^2.2.44",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.7",
    "@types/slug": "^0.9.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-json-schema": "^1.5.0",
    "dotenv": "^5.0.1",
    "joi": "^13.0.1",
    "mocha": "^5.0.4",
    "prettier": "^1.11.1",
    "tcs": "^10.0.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.6.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
